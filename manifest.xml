<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="TaskPaneApp">
  <Id>28477825-5554-4111-9edf-b1366c316ff9</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>JIRVentures</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="JIRView Add-in"/>
  <Description DefaultValue="Connect Your Own OLAP with Excel using JIRView."/>
  <IconUrl DefaultValue="https://localhost:3000/assets/icon-32.png"/>
  <HighResolutionIconUrl DefaultValue="https://localhost:3000/assets/icon-64.png"/>
  <SupportUrl DefaultValue="https://www.jirventures.com/contact"/>
  <AppDomains>
    <AppDomain>https://www.jirventures.com</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Workbook"/>
  </Hosts>
  <Requirements>
    <Sets DefaultMinVersion="1.1">
      <Set Name="SharedRuntime" MinVersion="1.1"/>
      <Set Name="ExcelApi" MinVersion="1.1"/>
    </Sets>
  </Requirements>
  <DefaultSettings>
    <SourceLocation DefaultValue="https://localhost:3000/taskpane.html"/>
  </DefaultSettings>
  <Permissions>ReadWriteDocument</Permissions>
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Hosts>
      <Host xsi:type="Workbook">
        <Runtimes>
          <Runtime resid="Taskpane.Url" lifetime="long"/>
        </Runtimes>
        <AllFormFactors>
          <ExtensionPoint xsi:type="CustomFunctions">
            <Script>
              <SourceLocation resid="Functions.Script.Url"/>
            </Script>
            <Page>
              <SourceLocation resid="Taskpane.Url"/>
            </Page>
            <Metadata>
              <SourceLocation resid="Functions.Metadata.Url"/>
            </Metadata>
            <Namespace resid="Functions.Namespace"/>
          </ExtensionPoint>
        </AllFormFactors>
        <DesktopFormFactor>
          <GetStarted>
            <Title resid="GetStarted.Title"/>
            <Description resid="GetStarted.Description"/>
            <LearnMoreUrl resid="GetStarted.LearnMoreUrl"/>
          </GetStarted>
          <FunctionFile resid="Commands.Url"/>
          <ExtensionPoint xsi:type="PrimaryCommandSurface">
            <!-- Add custom JIR Portal tab -->
            <CustomTab id="JIRPortal">
              <Group id="JIRPortalGroup">
                <Label resid="JIRPortal.Group.Label"/>
                <Icon>
                  <bt:Image size="16" resid="Icon.16x16"/>
                  <bt:Image size="32" resid="Icon.32x32"/>
                  <bt:Image size="80" resid="Icon.80x80"/>
                </Icon>
                <!-- Add Show Settings button -->
                <Control xsi:type="Button" id="TaskpaneButton">
                  <Label resid="TaskpaneButton.Label"/>
                  <Supertip>
                    <Title resid="TaskpaneButton.Label"/>
                    <Description resid="TaskpaneButton.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.16x16"/>
                    <bt:Image size="32" resid="Icon.32x32"/>
                    <bt:Image size="80" resid="Icon.80x80"/>
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <TaskpaneId>ButtonId1</TaskpaneId>
                    <SourceLocation resid="Taskpane.Url"/>
                  </Action>
                </Control>
                <!-- Existing Refresh Data button -->
                <!-- <Control xsi:type="Button" id="RefreshDataButton">
                  <Label resid="RefreshData.Label"/>
                  <Supertip>
                    <Title resid="RefreshData.Label"/>
                    <Description resid="RefreshData.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Refresh.16x16"/>
                    <bt:Image size="32" resid="Refresh.32x32"/>
                    <bt:Image size="80" resid="Refresh.80x80"/>
                  </Icon>
                  <Action xsi:type="ExecuteFunction">
                    <FunctionName>refreshData</FunctionName>
                  </Action>
                </Control> -->
                <!-- Add Refresh Automatically button -->
                <!-- <Control xsi:type="Button" id="RefreshAutoButton">
                  <Label resid="RefreshAuto.Label"/>
                  <Supertip>
                    <Title resid="RefreshAuto.Label"/>
                    <Description resid="RefreshAuto.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Refresh.16x16"/>
                    <bt:Image size="32" resid="Refresh.32x32"/>
                    <bt:Image size="80" resid="Refresh.80x80"/>
                  </Icon>
                  <Action xsi:type="ExecuteFunction">
                    <FunctionName>refresh_auto</FunctionName>
                  </Action>
                </Control> -->
              </Group>
              <Label resid="JIRPortal.Tab.Label"/>
            </CustomTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    <Resources>
      <bt:Images>
        <!-- Add refresh icon resources -->
        <!-- <bt:Image id="Refresh.16x16" DefaultValue="https://localhost:3000/assets/refresh-16.png"/>
        <bt:Image id="Refresh.32x32" DefaultValue="https://localhost:3000/assets/refresh-32.png"/>
        <bt:Image id="Refresh.80x80" DefaultValue="https://localhost:3000/assets/refresh-80.png"/> -->
        <bt:Image id="Icon.16x16" DefaultValue="https://localhost:3000/assets/icon-16.png"/>
        <bt:Image id="Icon.32x32" DefaultValue="https://localhost:3000/assets/icon-32.png"/>
        <bt:Image id="Icon.80x80" DefaultValue="https://localhost:3000/assets/icon-80.png"/>
      </bt:Images>
      <bt:Urls>
        <bt:Url id="Functions.Script.Url" DefaultValue="https://localhost:3000/functions.js"/>
        <bt:Url id="Functions.Metadata.Url" DefaultValue="https://localhost:3000/functions.json"/>
        <bt:Url id="GetStarted.LearnMoreUrl" DefaultValue="https://go.microsoft.com/fwlink/?LinkId=276812"/>
        <bt:Url id="Taskpane.Url" DefaultValue="https://localhost:3000/taskpane.html"/>
        <bt:Url id="Commands.Url" DefaultValue="https://localhost:3000/commands.js"/>
      </bt:Urls>
      <bt:ShortStrings>
        <!-- Add new string resources -->
        <bt:String id="Functions.Namespace" DefaultValue="JIRVIEW"/>
        <bt:String id="GetStarted.Title" DefaultValue="Get started with JIRView add-in!"/>
        <bt:String id="CommandsGroup.Label" DefaultValue="Commands Group"/>
        <bt:String id="TaskpaneButton.Label" DefaultValue="Show Task Pane"/>
        <bt:String id="JIRPortal.Tab.Label" DefaultValue="JIRView"/>
        <bt:String id="JIRPortal.Group.Label" DefaultValue="Data Operations"/>
        <!-- <bt:String id="RefreshData.Label" DefaultValue="Refresh Data"/>
        <bt:String id="RefreshAuto.Label" DefaultValue="Refresh Automatically"/> -->
      </bt:ShortStrings>
      <bt:LongStrings>
        <!-- Add new tooltip -->
        <bt:String id="GetStarted.Description" DefaultValue="JIRView add-in loaded successfully. Go to the JIRView tab and click the 'Show Task Pane' button to get started."/>
        <bt:String id="TaskpaneButton.Tooltip" DefaultValue="Click to Show a Task Pane"/>
        <!-- <bt:String id="RefreshData.Tooltip" DefaultValue="Refresh data from the selected range"/>
        <bt:String id="RefreshAuto.Tooltip" DefaultValue="Automatically refresh data by detecting the range and metadata"/> -->
      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>
